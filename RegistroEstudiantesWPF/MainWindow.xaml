<Window x:Class="RegistroEstudiantesWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegistroEstudiantesWPF"
        mc:Ignorable="d"
        Title="Ventana Principal" Height="200" Width="450">
    <Grid>
        <StackPanel>
            <Menu Height="18">
                <MenuItem Header="Registros">
                    <MenuItem x:Name="EstudianteMenuItem" Header="Registro Estudiante" Click="EstudianteMenuItem_Click"></MenuItem>
                    <MenuItem x:Name="RolesMenuItem" Header="Registro Roles" Click="RolesMenuItem_Click"></MenuItem>
                </MenuItem>
            </Menu>
        </StackPanel>
    </Grid>
</Window>
